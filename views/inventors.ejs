<!DOCTYPE html>
<html>
  <head>
    <title>Inventors</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
      <h2> List of Inventors </h2> <hr />
      <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th> 
            <th>Year</th>
          </tr>
          <% for(var i=0; i < inventors.length; i++) { %>
            <tr>
              <td><%= inventors[i].first %></td>
              <td><%= inventors[i].last %></td>
              <td><%= inventors[i].year %></td>
            </tr>
          <% } %>     
      </table>
  </body>
</html>
